<%= form_for @internship_recruitment do |f| %>
  <%= render 'shared/error_messages', object: @internship_recruitment %>

  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>

  <%= f.label :content %>
  <%= f.text_area :content, class: 'form-control' %>


  <%= f.label :required_qualifications %>
  <%= f.text_area :required_qualifications, class: 'form-control' %>


  <%= f.label :prefered_qualifications %>
  <%= f.text_area :prefered_qualifications, class: 'form-control' %>


  <%= f.label :benefits %>
  <%= f.text_area :benefits, class: 'form-control' %>

  <%= f.label :company %>
  <%= f.text_area :company, class: 'form-control' %>

  <%= f.submit "Save" , class: "btn btn-primary" %>
<% end %>